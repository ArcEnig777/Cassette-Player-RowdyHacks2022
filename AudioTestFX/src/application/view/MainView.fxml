<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MainViewController">
   <children>
      <Button fx:id="pauseButton" layoutX="505.0" layoutY="444.0" mnemonicParsing="false" onAction="#pause" prefHeight="97.0" prefWidth="87.0">
         <graphic>
            <ImageView fitHeight="79.0" fitWidth="71.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="./application/images/pause.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="stopButton" graphicTextGap="0.0" layoutX="629.0" layoutY="446.0" mnemonicParsing="false" onAction="#stop">
         <graphic>
            <ImageView fitHeight="80.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="./application/images/stop.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="restartButton" layoutX="876.0" layoutY="566.0" mnemonicParsing="false" onAction="#restart" text="Restart" />
      <Button fx:id="chooseFileButton" layoutX="547.0" layoutY="345.0" mnemonicParsing="false" onAction="#openFile" text="Choose Directory" />
      <Button fx:id="playButton" layoutX="396.0" layoutY="448.0" mnemonicParsing="false" onAction="#play">
         <graphic>
            <ImageView fitHeight="84.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="./application/images/play.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <ListView fx:id="list" layoutX="227.0" layoutY="35.0" onMouseClicked="#playSong" prefHeight="297.0" prefWidth="809.0" />
      <Button layoutX="275.0" layoutY="591.0" mnemonicParsing="false" onAction="#openLoop" text="Start Loop" />
      <Button layoutX="445.0" layoutY="591.0" mnemonicParsing="false" onAction="#closeLoop" text="End Loop" />
      <Button layoutX="352.0" layoutY="591.0" mnemonicParsing="false" onAction="#stopLooping" text="Stop Looping" />
      <ImageView fitHeight="82.0" fitWidth="66.0" layoutX="1035.0" layoutY="367.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="./application/images/backward.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="84.0" fitWidth="71.0" layoutX="1106.0" layoutY="367.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="./application/images/forward.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
